{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "tokens": {
      "items": {
        "properties": {
          "address": {
            "pattern": "^0x[a-fA-F0-9]{40}$",
            "type": "string"
          },
          "decimals": { "type": "number" },
          "image": { "type": "string" },
          "symbol": { "type": "string" },
          "tags": {
            "items": { "type": "string" },
            "type": "array"
          }
        },
        "required": ["address", "decimals", "image", "symbol"],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": ["tokens"],
  "type": "object"
}
